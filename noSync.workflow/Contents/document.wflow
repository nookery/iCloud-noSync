				<key>ActionParameters</key>
				<dict>
					<key>COMMAND_STRING</key>
					<string>no_sync () {
  #Stopping iCloud sync for $1

  mv "$1" "$1.nosync"
  #Moved $1 to $1.nosync

  ln -s "$1.nosync" "$1"
  #Symbolic link created for $1.nosync -&gt; $1
}

until [ -z "$1" ]  # Until all parameters used up.
do
  no_sync $1
  shift # Shift param n+1 into $1.
done</string>
